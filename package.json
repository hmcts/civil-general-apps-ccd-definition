{
  "name": "civil-ga-ccd-definition",
  "version": "0.0.1",
  "description": "Civil General apps e2e tests",
  "scripts": {
    "lint": "eslint e2e",
    "test": "npx codeceptjs run --steps",
    "test:smoke": "MOCHAWESOME_REPORTFILENAME=smoke REPORT_DIR=test-results/smokeTest REPORT_FILE=test-results/smokeTest/results.xml npx codeceptjs run --grep @smoke-tests --reporter mocha-multi --verbose",
    "test:ga-smoke": "MOCHAWESOME_REPORTFILENAME=ga-smoke REPORT_DIR=test-results/smokeTest REPORT_FILE=test-results/smokeTest/results.xml npx codeceptjs run --grep @ga-smoke-tests --reporter mocha-multi --verbose",
    "test:api": "MOCHAWESOME_REPORTFILENAME=api npx codeceptjs run --grep @api-tests --reporter mocha-multi --verbose",
    "test:e2e": "MOCHAWESOME_REPORTFILENAME=e2e npx codeceptjs run --grep @e2e-tests --reporter mocha-multi --verbose",
    "test:e2e-multiparty": "MOCHAWESOME_REPORTFILENAME=e2e-multiparty npx codeceptjs run --grep @multiparty-e2e-tests --reporter mocha-multi --verbose",
    "test:functional": "MOCHAWESOME_REPORTFILENAME=functional npx codeceptjs run --grep '@e2e-tests|@api-tests' --reporter mocha-multi --verbose",
    "test:crossbrowser-e2e": "MOCHAWESOME_REPORTFILENAME=crossbrowser codeceptjs run-multiple --grep @cross-browser-tests ${BROWSER_GROUP:-'--all'} -c saucelabs.conf.js --reporter mocha-multi",
    "test:crossbrowser": "./e2e/run-crossbrowser-tests.sh",
    "test:fullfunctional": "MOCHAWESOME_REPORTFILENAME=functional npx codeceptjs run --grep '@e2e-tests|@multiparty-e2e-tests' --reporter mocha-multi --verbose",
    "prepare": "husky install"
  },
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.3.4",
    "codeceptjs": "^3.3.5",
    "deep-equal-in-any-order": "^1.0.28",
    "eslint": "^8.20.0",
    "eslint-plugin-codeceptjs": "^1.3.0",
    "faker": "^6.6.6",
    "html_codesniffer": "^2.5.1",
    "husky": "^7.0.2",
    "lodash": "^4.17.21",
    "mocha": "9.2.2",
    "mocha-junit-reporter": "^2.0.2",
    "mocha-multi": "^1.1.6",
    "mochawesome": "^7.0.1",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.7",
    "puppeteer": "14.0.0",
    "totp-generator": "^0.0.7",
    "uuid": "^3.4.0",
    "webdriverio": "^6.12.1"
  },
  "resolutions": {
    "**/**/ansi-regex": "^5.0.1",
    "**/**/follow-redirects": "^1.14.8",
    "**/sass-lint/**/minimist": "^1.2.5"
  }
}
