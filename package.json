{
  "name": "civil-ga-ccd-definition",
  "version": "0.0.1",
  "description": "Civil General apps e2e tests",
  "scripts": {
    "lint": "eslint e2e",
    "setup": "echo yarn:setup",
    "test": "NODE_PATH=. mocha e2e/tests/unit/definitions/**/*.js",
    "test:win32": "mocha e2e/tests/unit/definitions/**/*.js",
    "test:a11y": "echo test:a11y",
    "test:coverage": "echo test:coverage",
    "highLevelDataSetup": "echo './gradlew highLevelDataSetup --args=$1' > ./temp.sh && sh ./temp.sh",
    "test:smoke": "run-script-os",
    "test:smoke:win32": "yarn playwright install && npx codeceptjs run --grep @smoke-tests --reporter mocha-multi --verbose",
    "test:smoke:darwin:linux": "MOCHAWESOME_REPORTFILENAME=smoke REPORT_DIR=test-results/smokeTest REPORT_FILE=test-results/smokeTest/results.xml yarn playwright install && npx codeceptjs run --grep @smoke-tests --reporter mocha-multi --verbose",
    "test:ga-smoke": "yarn playwright install && run-script-os",
    "test:ga-smoke:win32": "npx codeceptjs run --grep @ga-smoke-tests --reporter mocha-multi --verbose",
    "test:ga-smoke:darwin:linux": "MOCHAWESOME_REPORTFILENAME=ga-smoke REPORT_DIR=test-results/smokeTest REPORT_FILE=test-results/smokeTest/results.xml npx codeceptjs run --grep @ga-smoke-tests --reporter mocha-multi --verbose",
    "test:api": "run-script-os",
    "test:api:win32": "FUNCTIONAL=true npx codeceptjs run-workers --suites 5 --grep @api-tests --reporter mocha-multi --verbose",
    "test:api:darwin:linux": "MOCHAWESOME_REPORTFILENAME=api FUNCTIONAL=true npx codeceptjs run-workers --suites 5 --grep '@api-tests' --reporter mocha-multi --verbose",
    "test:e2e-nonprod": "MOCHAWESOME_REPORTFILENAME=e2e-preview-nonprod FUNCTIONAL=true npx codeceptjs  run-workers --suites 10 --grep '@e2e-nonprod|@e2e-tests|@api-nonprod|@api-tests' --reporter mocha-multi --verbose",
    "test:api-nonprod": "MOCHAWESOME_REPORTFILENAME=api-preview-nonprod FUNCTIONAL=true npx codeceptjs  run-workers --suites 7 --grep '@api-nonprod|@api-tests' --reporter mocha-multi --verbose",
    "test:e2e": "MOCHAWESOME_REPORTFILENAME=e2e npx codeceptjs run --grep @e2e-tests --reporter mocha-multi --verbose",
    "test:e2e-multiparty": "MOCHAWESOME_REPORTFILENAME=e2e-multiparty npx codeceptjs run --grep @multiparty-e2e-tests --reporter mocha-multi --verbose",
    "test:functional": "./e2e/run-functional-tests.sh",
    "test:functionallocal": "run-script-os",
    "test:functionallocal:win32": "npx codeceptjs run --grep \"@e2e-tests|@api-tests\" --reporter mocha-multi --verbose",
    "test:functionallocal:darwin:linux": "MOCHAWESOME_REPORTFILENAME=functional npx codeceptjs run --grep '@e2e-tests|@api-tests' --reporter mocha-multi --verbose",
    "test:crossbrowser-e2e": "MOCHAWESOME_REPORTFILENAME=crossbrowser codeceptjs run-multiple --grep @cross-browser-tests ${BROWSER_GROUP:-'--all'} -c saucelabs.conf.js --reporter mocha-multi",
    "test:crossbrowser": "./e2e/run-crossbrowser-tests.sh",
    "test:e2e-ga-wa": "MOCHAWESOME_REPORTFILENAME=wa-ui npx codeceptjs run-workers --suites 4 --grep @e2e-wa --reporter mocha-multi --verbose",
    "test:api-ga-wa": "MOCHAWESOME_REPORTFILENAME=wa-api npx codeceptjs run-workers --suites 4 --grep @api-wa --reporter mocha-multi --verbose",
    "test:nightly-api-tests": "MOCHAWESOME_REPORTFILENAME=api-regression-tests PROCEED_ON_FAILURE=true FUNCTIONAL=true npx codeceptjs run-workers --suites 4 --grep '@api-nightly|@api-scheduler-test|@api-tests' --reporter mocha-multi --verbose",
    "test:regression-e2e-tests": "MOCHAWESOME_REPORTFILENAME=ui-regression-tests FUNCTIONAL=true PROCEED_ON_FAILURE=true npx codeceptjs run-workers --suites 6 --grep @regression --reporter mocha-multi --verbose",
    "test:master-e2e-tests": "MOCHAWESOME_REPORTFILENAME=master-e2e-tests FUNCTIONAL=true npx codeceptjs run-workers --suites 10 --grep '@e2e-tests|@api-tests' --reporter mocha-multi --verbose",
    "test:fullfunctional": "./e2e/run-full-functional-tests.sh",
    "prepare": "husky install",
    "yarn-update": "yarn set version 3.x",
    "playwright-install": "yarn playwright install",
    "install-dependencies": "/usr/bin/yarn install --immutable --silent"
  },
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.3.4",
    "codeceptjs": "^3.3.7",
    "deep-equal-in-any-order": "^1.0.28",
    "eslint": "^8.20.0",
    "eslint-plugin-codeceptjs": "^1.3.0",
    "faker": "^6.6.6",
    "html_codesniffer": "^2.5.1",
    "husky": "^7.0.2",
    "lodash": "^4.17.21",
    "mocha": "10.2.0",
    "mocha-junit-reporter": "^2.0.2",
    "mocha-multi": "^1.1.6",
    "mochawesome": "^7.0.1",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.7",
    "playwright": "^1.41.1",
    "proper-lockfile": "^4.1.2",
    "run-script-os": "^1.1.6",
    "totp-generator": "^0.0.7",
    "uuid": "^3.4.0",
    "webdriverio": "^6.12.1"
  },
  "packageManager": "yarn@3.3.1",
  "dependencies": {
    "@codeceptjs/helper": "^2.0.1"
  }
}
